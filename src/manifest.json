{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "http://eastmoney.com/",
    "http://fund.eastmoney.com/*",
    "http://fundsuggest.eastmoney.com/*"
  ],
  "icons": {
    "16": "icons/mountain_16.png",
    "48": "icons/mountain_48.png",
    "128": "icons/mountain_128.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "24": "icons/mountain_24.png",
      "48": "icons/mountain_48.png"
    }
  }
}
